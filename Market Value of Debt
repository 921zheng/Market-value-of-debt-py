      "source": [
        "principal = 300000\n",
        "coupon_rate = 0.07\n",
        "maturity = 2\n",
        "\n",
        "coupon_payment = coupon_rate * principal\n",
        "cash_flows = [coupon_rate * principal for i in range(maturity - 1)] + [(1 + coupon_rate) * principal]\n",
